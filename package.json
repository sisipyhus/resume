  "version": "2.0.0",
  "description": "Resume generator by Gulp + Sass",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp clean && gulp dev",
    "pdf": "gulp pdf",
    "dev": "gulp dev",
